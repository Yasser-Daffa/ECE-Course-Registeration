        <widget class="QScrollArea" name="navScrollArea">
         <property name="styleSheet">
          <string notr="true">QScrollArea { background-color: white; border: none; }

/* ----------------- Modern Scrollbar with Embedded SVG Arrows ----------------- */
/* Colors you can tweak */
@declared-track: #eef3fa;    /* track background */
@declared-handle: #c7ccd1;   /* handle normal */
@declared-handle-hover: #b6bcc2;
@declared-handle-pressed: #9fa7b0;
@declared-arrow-color: #7c8691;
@declared-size: 12px;

/* ---------- Vertical scrollbar ---------- */
QScrollBar:vertical {
    background: transparent;
    width: @declared-size;
    margin: 12px 0 12px 0; /* space for arrows */
}

/* track (page area) */
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
    background: transparent;
}

/* handle */
QScrollBar::handle:vertical {
    background: @declared-handle;
    min-height: 28px;
    border-radius: 6px;
    margin: 2px 2px; /* keep small gap from edges */
}
QScrollBar::handle:vertical:hover { background: @declared-handle-hover; }
QScrollBar::handle:vertical:pressed { background: @declared-handle-pressed; }

/* arrows - sizes set to match the scrollbar width */
QScrollBar::sub-line:vertical, QScrollBar::add-line:vertical {
    height: 12px;
    subcontrol-origin: margin;
    subcontrol-position: top;
    background: transparent;
    border: none;
}

/* keep area clickable and subtly highlight on hover */
QScrollBar::sub-line:vertical:hover,
QScrollBar::add-line:vertical:hover {
    background: rgba(0,0,0,0.03);
    border-radius: 4px;
}

/* ---------- Horizontal scrollbar ---------- */
QScrollBar:horizontal {
    background: transparent;
    height: @declared-size;
    margin: 0 12px 0 12px;
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal {
    background: transparent;
}
QScrollBar::handle:horizontal {
    background: @declared-handle;
    min-width: 28px;
    border-radius: 6px;
    margin: 2px 2px;
}
QScrollBar::handle:horizontal:hover { background: @declared-handle-hover; }
QScrollBar::handle:horizontal:pressed { background: @declared-handle-pressed; }

/* arrows for horizontal */
QScrollBar::sub-line:horizontal, QScrollBar::add-line:horizontal {
    width: 12px;
    subcontrol-origin: margin;
    subcontrol-position: left;
    background: transparent;
    border: none;
}
QScrollBar::sub-line:horizontal:hover,
QScrollBar::add-line:horizontal:hover {
    background: rgba(0,0,0,0.03);
    border-radius: 4px;
}

/* ----------------- CUSTOM SVG ARROWS (embedded) -----------------
   We use small inline SVGs encoded as UTF-8 (URL-escaped).
   Change @declared-arrow-color if you want different arrow color.
   ---------------------------------------------------------------------------- */

/* up arrow (vertical sub-line) */
QScrollBar::sub-line:vertical {
    image: url(&quot;data:image/svg+xml;utf8,\
    &lt;svg xmlns='http://www.w3.org/2000/svg' width='10' height='10' viewBox='0 0 10 10'&gt;\
    &lt;polygon points='5,2 9,8 1,8' fill='%237c8691'/&gt;\
    &lt;/svg&gt;&quot;);
    subcontrol-position: top;
    subcontrol-origin: margin;
}

/* down arrow (vertical add-line) */
QScrollBar::add-line:vertical {
    image: url(&quot;data:image/svg+xml;utf8,\
    &lt;svg xmlns='http://www.w3.org/2000/svg' width='10' height='10' viewBox='0 0 10 10'&gt;\
    &lt;polygon points='1,2 9,2 5,8' fill='%237c8691'/&gt;\
    &lt;/svg&gt;&quot;);
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

/* left arrow (horizontal sub-line) */
QScrollBar::sub-line:horizontal {
    image: url(&quot;data:image/svg+xml;utf8,\
    &lt;svg xmlns='http://www.w3.org/2000/svg' width='10' height='10' viewBox='0 0 10 10'&gt;\
    &lt;polygon points='8,1 8,9 2,5' fill='%237c8691'/&gt;\
    &lt;/svg&gt;&quot;);
}

/* right arrow (horizontal add-line) */
QScrollBar::add-line:horizontal {
    image: url(&quot;data:image/svg+xml;utf8,\
    &lt;svg xmlns='http://www.w3.org/2000/svg' width='10' height='10' viewBox='0 0 10 10'&gt;\
    &lt;polygon points='2,1 8,5 2,9' fill='%237c8691'/&gt;\
    &lt;/svg&gt;&quot;);
}

/* ----------------- Optional: make arrows a bit brighter on hover ----------------- */
QScrollBar::sub-line:vertical:hover image,
QScrollBar::add-line:vertical:hover image,
QScrollBar::sub-line:horizontal:hover image,
QScrollBar::add-line:horizontal:hover image {
    /* re-specify with a slightly darker color */
    image: url(&quot;data:image/svg+xml;utf8,\
    &lt;svg xmlns='http://www.w3.org/2000/svg' width='10' height='10' viewBox='0 0 10 10'&gt;\
    &lt;polygon points='5,2 9,8 1,8' fill='%236a7480'/&gt;\
    &lt;/svg&gt;&quot;);
}</string>
